<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.immoxz.run">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HelloScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TrainingActivity" />
        <activity android:name=".Statistisc" />

        <service
            android:name=".MyRunService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name=".MyWalkService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name=".MyCycleService"
            android:enabled="true"
            android:exported="false"></service>
    </application>

</manifest>